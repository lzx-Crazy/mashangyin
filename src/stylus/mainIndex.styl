      margin Rem(10) auto
      font-size: 0
      text-align: center
      span
        font-size: Rem(10)
      .mifmb-link-box
        display inline-block
        vertical-align top
        font-size: Rem(10)
        width 70%
        text-align left 
        >a
          color: $theme-color
          border-right 1px solid $theme-color
          margin-right Rem(2)
          margin-bottom Rem(5)
          display inline-block
          padding-right Rem(3)
          height Rem(10)
          line-height: Rem(10)
          &:last-child
            border-right 0
    .mifm-bztxt
      border-top: 1px solid #ccc
      position relative
      padding Rem(20)
      margin 0 Rem(5)
      >p
        position absolute
        top Rem(-7)
        width Rem(220)
        text-align center
        left 50%
        margin-left Rem(-110)
        color: #999
        font-size: 12px
        background-color #f4f4f4

.mi-td
  width Rem(45)
  height Rem(45)
  line-height Rem(45)
  text-align center
  border-radius: 50%
  background-color #fff
  border Rem(1) solid $theme-color
  position fixed
  left 0
  top 42%
  color: $theme-color
  font-weight: bold
  font-size: Rem(18)
  z-index 9
.mi-hybox
  width 100%
  height Rem(45)
  border-top-left-radius: Rem(45)
  border-bottom-left-radius: Rem(45)
  background-color rgba(220,55,84,0.9)
  position fixed
  right -90%
  top: 55%
  z-index 9
  .mih-jtICon
    getBg("dajiantou")
    background-size contain
    height Rem(20)
    width Rem(25/2)
    position fixed
    display: block
    top: 55%
    right Rem(15)
    margin-top Rem(12)
    transition: .3s ease
    &.on
      transition: .3s ease
      transform rotate(180deg)
  .mih-section
    display flex
    justify-content: space-between
    align-items: center
    padding 0 Rem(30) 0 Rem(45)
    height Rem(45)
    font-size Rem(10)
    color #fff
    >div
      width 45%
      &.mihs-page
        border-right 1px solid #eee
      p
        font-size: Rem(14)
        padding-bottom Rem(5)
      em 
        font-size: Rem(14)
        font-weight: bold
      span
        padding-left Rem(10)
      .mihs-jtIcon
        getBg("shangtou")
        width Rem(15/2)
        height Rem(10)
        background-size contain
        display inline-block
      