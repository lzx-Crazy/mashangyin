    .mizn-msgbox
      margin-top Rem(5)
      li
        margin 0 Rem(5) Rem(5)
        padding Rem(10) 0
        background-color #fff
        font-size: Rem(10)
        color: #666
        .mil-page
          display flex
          justify-content space-around
          align-items: center
          text-align: right
          >em
            width 60%
            ellp()
            text-align left 
          .mil-bor
            border 1px solid #ccc
            padding Rem(2) Rem(5)
            border-radius: Rem(4)
        .mil-ck
          display flex
          justify-content: space-between
          align-items: center
          margin Rem(30/2) 0
          p
            width 33.333%
            text-align center
            height Rem(35/2)
            line-height Rem(35/2)
            font-size: Rem(15)
            color: #666
            &.br
              border-right 1px solid #eee
            &.money
              font-size: Rem(23)
              color: $theme-color
              text-align left
              padding-left Rem(10)
              span
                vertical-align middle
            .miz-yuan
              width Rem(20)
              height Rem(20)
              line-height Rem(20)
              text-align center
              color: #999
              border 1px solid #999
              display inline-block
              vertical-align middle
              border-radius: 50%
              font-size: Rem(10)
        .mil-tiems
          text-align right
          padding-right Rem(10)
  .miz-lianxi
    padding Rem(10)
    background-color #fff
    border-top 1px solid #eee
    margin-bottom Rem(5)
    li
      margin-bottom Rem(5)
      font-size: Rem(12)
      ellp()
      em
        display inline-block
        vertical-align middle
        padding Rem(7) 0
        background-color $theme-color
        color: #fff
        border-radius: Rem(4)
        width Rem(75)
        text-align center
      span
        display inline-block
        vertical-align middle
        color: #666
        padding-left Rem(5)
  .miz-swiperbox-tow
    background-color #fff
    height Rem(200)
    font-size: 0
    padding-left 3%
    border-top 1px solid #eee
    >a
      display inline-block
      vertical-align middle
      font-size: Rem(10)
      width 30%
      margin-left 2%
      overflow hidden
      margin-top Rem(5)
      color: #999
      text-align center
      >p
        margin-top Rem(5)
      .mizs-img
        border: 1px solid #eee
        text-align center
        height Rem(75/2)
        line-height Rem(75/2)
        img
          max-width 80%
          max-height 80%
          display inline-block
          vertical-align middle
  .mi-foot-ms
    margin Rem(20) auto 0
    line-height Rem(15)
    .mifm-linkapp
      display block
    .mifm-img
      width Rem(30)
      margin 0 auto
      img
        width 100%
    .mifm-apptxt
      text-align center
      font-size: Rem(12)
      color: $theme-color
    .mifm-yl-box
