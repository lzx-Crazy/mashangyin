@import './base'

.main-index
  // 4个菜单入口
  .mi-cimenubox
    display flex
    justify-content space-between
    align-items: center
    overflow hidden
    background-color #fff
    padding Rem(15/2) 0
    a
      display block
      text-align: center
      width: 20%
      margin: 0 auto
      color: #666
      font-size: Rem(12)
      .mic-imgpage
        width Rem(50)
        height Rem(50)
        display flex
        align-items: center
        justify-content: center
        text-align: center
        margin 0 auto Rem(6)
        img
          max-width 100%
  // 价格
  .mi-moneybox
    display flex
    justify-content flex-start
    align-items center
    margin-top Rem(5)
    background-color #fff
    padding: Rem(15/2) 0
    botLine()
    span
      vertical-align middle
    .mim-page
      height Rem(40)
      line-height: Rem(40)
      width Rem(150)
      padding-right Rem(10)
      border-right: 1px solid #eee
      text-align right 
      font  
      min-width: Rem(150)
      .mim-nums
        color: $theme-color
        font-size: Rem(24)
        font-weight: bold
    .mim-jg
      display flex
      justify-content space-between
      align-items center
      color: #666
      flex 1
      .min-more
        color #999
        font-size: Rem(10)
      div
        vertical-align middle
        font-size: Rem(12)
        text-align: center
        padding-right Rem(5)
        min-width: Rem(60)
        p
          padding-bottom Rem(3)
        .min-weid
          font-size: Rem(18)
          font-weight: bold
          color: #666
        .min-green,.lv
          color: #80c269
        .min-red,.red
          color: $theme-color
  .mi-zxbox
    height Rem(40)
    background-color #fff
    color: #666
    display flex
    align-items: center
    justify-content flex-start
    botLine()
    .miz-qian
      width Rem(85)
      text-align right 
      font-weight: bold
      font-size: Rem(15)
      span
        vertical-align middle
    .miz-zxicon
      display inline-block
      width Rem(41/2)
      height Rem(37/2)
      getBg("zixun")
      background-size contain
      vertical-align middle
    .notice-con
      height Rem(40)
      overflow hidden
      .notice-txt
        li
          height Rem(40)
          line-height: Rem(40)
          ellp()
          padding: 0 Rem(15) 0 Rem(5) 
          a
            color: #666
            font-size: Rem(12)
  .mi-zybox
    .miz-title
      height Rem(40)
      line-height: Rem(40)
      background-color #fff
      toMore(15)
      padding 0 Rem(15)
      color: #666
      font-size: Rem(15)
      .mizt-bl
        padding-left Rem(10)
        border-left 3px solid $theme-color
      .mizt-more
        float right
        padding-right Rem(15)
        font-size: Rem(12)
        >a
          color #666
        .mizt-qieh
          display inline-block
          vertical-align middle
          background-color $theme-color
          color $theme-color
          height Rem(26)
          line-height Rem(26)
          border-radius: Rem(30)
          padding Rem(2) Rem(2) 0
          margin-right Rem(5)
          font-size: 0
          >span
            font-size: Rem(10)
            width: Rem(24)
            text-align: center
            height Rem(24)
            line-height Rem(24)
            margin: 0
            border-radius: Rem(24)
            display inline-block
            &.active
              background-color #fff
              color $theme-color
      &.miz-title-nomore
        height Rem(40)
        line-height: Rem(40)
        background-color #fff
        padding 0 Rem(15)
        color: #666
        font-size: Rem(15)
        &:after
          display none
    .miz-swiperbox
      margin Rem(5) 0
      width 100%
      overflow hidden
      .mizt-box
        background-color #fff
        border-radius: Rem(4)
        padding: Rem(5) 0
        margin 0 Rem(3)
        height Rem(230)
        overflow hidden
        .mizt-title
          padding Rem(5)
          background-color $theme-color
          color: #fff
          display inline-block
          font-size: Rem(10)
          margin-left Rem(5)
        .mizt-ptpage
          text-align center
          font-size: Rem(14)
          color: #666
          line-height: Rem(15)
          .mizt-slbox
            margin-top Rem(5)
            p
              font-size: Rem(10)
          .mizt-qh
            padding Rem(5) 0 Rem(6)
          .mizt-ck
            padding-bottom: Rem(6)
            .mizz-weizhiIcon
              getBg("dingwei")
              width Rem(7)
              height Rem(21/2)
              background-size contain
              display inline-block
              vertical-align middle
              margin-right Rem(3)
          .mizt-cj
            color: $theme-color
        .mizt-btnbox
          .mizb-btn
            width 85%
            height Rem(25)
            line-height Rem(25)
            background-color #fff
            border 1px solid $theme-color
            color: $theme-color
            text-align center
            font-size: Rem(12)
            border-radius: Rem(4)
            display block
            margin Rem(15/2) auto 0
            &.on
              background-color $theme-color
              color #fff
            &.miz-hui
              background-color #ccc
              border 1px solid #ccc
              color #fff
        .mizt-times
          color: #999
          font-size: Rem(12)
          text-align center
          padding-top Rem(7)
    .mizn-listbox
      padding-top Rem(5)
      li
        background-color #fff
        border-radius Rem(4)
        margin 0 Rem(3) Rem(5)
        padding Rem(10) 0
        display flex
        justify-content: space-between
        align-items center
        text-align: center
        font-size: Rem(10)
        color: #666
        >div
          border-right 1px solid #eee
          height Rem(40)
          width 21%
          overflow hidden
          &:last-child
            border-right 0
          p
            line-height Rem(17)
            ellp()
        .miz-ckbox
          width 15%
        .miz-zf
          width 17%
          .miz-yp
            border: 1px solid #999
            padding Rem(3)
            display block
            width 75%
            margin 0 auto Rem(5)
            border-radius: Rem(4)
            font-size: Rem(10)
            ellp()
          .miz-zficon
            getBg("zhuanfa")
            background-size contain
            display inline-block
            width Rem(29/2)
            height Rem(10)
            margin-right Rem(2)
          a
            color #666
        .mizl-zs
          padding-left: Rem(4)
          border-right 1px solid #eee
          line-height Rem(35)
          width 23%
          text-align left 
          ellp()
          .miz-yuan
            width Rem(20)
            height Rem(20)
            line-height Rem(20)
            text-align center
            color: #999
            border 1px solid #999
            display inline-block
            vertical-align middle
            border-radius: 50%
          .miz-jgs
            font-size: Rem(14)
            vertical-align middle
            &.miz-hyzl
              font-size: Rem(10)
            .miz-lv
              color: #80c269
            .miz-red
              color: $theme-color
        .miz-gm
          display inline-block
          background-color $theme-color
          color: #ffffff
          width 80%
          height Rem(45/2)
          line-height Rem(47/2)
          border-radius: Rem(4)
          &.miz-hui
            background-color #ccc
        .mizl-jk-page
          width 50%
          padding 0 2%
          .mijk-head
            display flex
            justify-content: space-between
            align-items: center
            botLine()
            text-align center
            >div 
              border-right 1px solid #eee
              width 33.33%
              &:last-child
                border-right 0
          .mijk-foot
            ellp()
            line-height Rem(18)
